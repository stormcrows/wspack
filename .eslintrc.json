{
  "extends": "eslint:recommended",
  "rules": {
    "quotes": ["error", "double"],
    "no-console": "off",
    "no-unused-vars": "off"
  },
  "env": {
    "jest": true,
    "commonjs": true,
    "es6": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6
  }
}
